{
    "manifest_version": 3,
    "name": "Simple Time Tracker",
    "version": "1.0",
    "permissions": ["tabs", "activeTab"],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html"
    },
    "host_permissions": ["<all_urls>"]
  }
  